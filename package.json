{
  "name": "@jvvan/discord.js-menu",
  "version": "2.0.0",
  "description": "A utility for creating interactive menus with discord.js",
  "main": "dist/cjs/index.cjs",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.mjs",
  "exports": {
    "import": {
      "types": "./dist/esm/index.d.mts",
      "default": "./dist/esm/index.mjs"
    },
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvvan/discord.js-menu.git"
  },
  "keywords": [
    "discord.js"
  ],
  "author": "jvvan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jvvan/discord.js-menu/issues"
  },
  "homepage": "https://github.com/jvvan/discord.js-menu#readme",
  "devDependencies": {
    "tsup": "^8.3.6",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "discord.js": "^14.18.0"
  }
}